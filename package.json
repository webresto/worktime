{
  "name": "@webresto/worktime",
  "repository": {
    "type": "git",
    "url": "https://github.com/webresto/worktime.git"
  },
  "version": "2.2.0",
  "scripts": {
    "pub": "cd dist && npm publish --access=public",
    "tsc:commonjs": "tsc -p tsconfig.commonjs.json",
    "build:esm": "ng-packagr -p ng-package.json",
    "fix-commonjs-build": "node fix-commonjs-build.js",
    "build": "npm run build:esm && npm run tsc:commonjs && npm run fix-commonjs-build && cp .npmrc dist",
    "test": "node ./run-tests-multi-tz.js"
  },
  "devDependencies": {
    "@angular/compiler": "^15.2.1",
    "@angular/compiler-cli": "^15.2.1",
    "@angular/core": "^15.2.1",
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.14.6",
    "chai": "^4.3.7",
    "mocha": "^10.2.0",
    "ng-packagr": "^15.2.2",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "contributors": [
    {
      "name": "axrl",
      "email": "axrl@mail.ru"
    },
    {
      "name": "Siziy Ivan",
      "email": "xziy88@gmail.com"
    }
  ],
  "bugs": {
    "url": "https://github.com/webresto/worktime/issues"
  },
  "homepage": "https://www.npmjs.com/package/@webresto/worktime",
  "license": "MIT",
  "description": "Library for working with enterprise working hours restrictions for @webresto",
  "dependencies": {
    "tslib": "^2.5.3"
  },
  "main": "dist/bundles/index.js",
  "module": "dist/fesm2015/webresto-worktime.mjs",
  "es2020": "dist/fesm2020/webresto-worktime.mjs",
  "esm2020": "dist/esm2020/webresto-worktime.mjs",
  "fesm2020": "dist/fesm2020/webresto-worktime.mjs",
  "fesm2015": "dist/fesm2015/webresto-worktime.mjs",
  "typings": "dist/index.d.ts",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "esm2020": "./dist/esm2020/webresto-worktime.mjs",
      "es2020": "./dist/fesm2020/webresto-worktime.mjs",
      "es2015": "./dist/fesm2015/webresto-worktime.mjs",
      "node": "./dist/bundles/index.js",
      "default": "./dist/fesm2020/webresto-worktime.mjs"
    }
  },
  "sideEffects": false
}
